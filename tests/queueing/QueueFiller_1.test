%file: test.ned

import inet.queueing.sink.ActivePacketSink;
import inet.queueing.queue.PacketQueue;
import inet.queueing.source.QueueFiller;

network TestQueueFiller
{
    submodules:
        filler: QueueFiller {
            @display("p=100,100");
        }
        queue: PacketQueue {
            @display("p=200,100");
        }
        consumer: ActivePacketSink {
            @display("p=300,100");
        }
    connections allowunconnected:
        filler.out --> queue.in;
        queue.out --> consumer.in;
}

%inifile: omnetpp.ini

[General]
network = TestQueueFiller
sim-time-limit = 10s
cmdenv-event-banners = false
cmdenv-log-prefix = "At %ts: "
*.filler.provider.packetLength = 1B
*.filler.tokenGenerator.queueModule = "queue"
*.consumer.collectionInterval = 1s

%# remove formatting
%subst: /\x1B\[[0-9;]*m//

